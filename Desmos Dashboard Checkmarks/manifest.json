{
	"name": "Desmos Dashboard Checkmarks",
	"description": "Show Checkmark Sums Desmos teacher dashboard. (unofficial)",
	"version": "1.0",
	"manifest_version": 3,
	"permissions": ["scripting", "activeTab"],
	"action": {
   		"default_title": "Desmos Dashboard Checkmarks",  
		"default_popup": "popup.html"
	},
	"icons": {
		"32": "icons/32x32.png",
		"64": "icons/64x64.png",
		"128": "icons/128x128.png"
	}
    
}
